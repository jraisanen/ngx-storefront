{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "ngx-core": {
      "projectType": "library",
      "root": "packages/core",
      "sourceRoot": "packages/core",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "tsconfig.json",
            "project": "packages/core/ng-package.json"
          }
        }
      }
    },
    "ngx-elements": {
      "projectType": "library",
      "root": "packages/elements",
      "sourceRoot": "packages/elements",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "tsconfig.json",
            "project": "packages/elements/ng-package.json"
          }
        }
      }
    },
    "ngx-layout": {
      "projectType": "library",
      "root": "packages/layout",
      "sourceRoot": "packages/layout",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
