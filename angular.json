        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "tsconfig.json",
            "project": "packages/storefront/ng-package.json"
          }
        }
      }
    }
  },
  "defaultProject": "ngx-core"
}
